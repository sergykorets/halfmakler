<ul id='rooms_ul'>
  <% rooms.each do |room| %>
    <%= link_to room_path(room), id: 'room_link' do %>
      <li id='room'>
          <div class="panel panel-default">
            <div class="panel-heading preview">
              <%= image_tag room.cover_photo(:medium) %>
            </div>
            <div class="panel-body">
              <%= link_to room.listing_name, room %><br/>
              <%= room.price %> - <%= room.home_type %> - <%= pluralize(room.bed_room, "bed") %>
            </div>
          </div>
      </li>
    <% end %>
  <% end %>
</ul>