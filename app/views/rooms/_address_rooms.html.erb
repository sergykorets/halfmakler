<div class="modal-content">
  <div class="modal-body" id='address_rooms'>
    <%- @rooms.each do |room| %>
      <%= link_to room_path(room), id: 'room_link' do %>
        <div class="panel panel-default">
          <div class="panel-heading preview">
            <%= image_tag room.cover_photo(:big) %>
          </div>
          <div class="panel-body">
            <%= link_to room.listing_name, room %><br/>
            $<%= room.price %> - <%= room.home_type %> - <%= pluralize(room.bed_room, "bed") %>
            <div id="star_<%= room.id %>"></div> <%= pluralize(room.average_rating, "review") %>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>